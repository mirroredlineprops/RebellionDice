<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.mirroredlineprops.alex.rebelliondice" >

    <application
        android:allowBackup="true"
        android:icon="@drawable/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name=".MainActivity"
            android:label="@string/app_name"
            android:theme="@android:style/Theme.Holo.Light" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".ExpandableListActivity"
            android:label="@string/title_activity_expandable_list"
            android:theme="@android:style/Theme.Holo.Light">
        </activity>
        <activity
            android:name=".DiceRoll"
            android:label="@string/title_activity_dice_roll"
            android:theme="@android:style/Theme.Holo.Light" >
        </activity>
    </application>

</manifest>
